{
    "name": "amphp/elasticsearch",
    "description": "An asynchronous Elasticsearch PHP client.",
    "require": {
        "php": "~7.1.0|~7.2.0|~7.3.0",
        "ext-json": "*",
        "amphp/artax": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Elasticsearch\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.1"
    }
}
