{
    "name": "webgriffe/amp-elasticsearch",
    "description": "A non-blocking ElasticSearch client for PHP based on Amp.",
    "require": {
        "php": "~7.1.0|~7.2.0|~7.3.0",
        "ext-json": "*",
        "amphp/artax": "^3.0",
        "amphp/amp": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Webgriffe\\AmpElasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webgriffe\\AmpElasticsearch\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.1"
    }
}
